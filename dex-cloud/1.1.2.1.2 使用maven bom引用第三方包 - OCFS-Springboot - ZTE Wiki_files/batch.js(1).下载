lterNoAutoescape(AJS.format("\u60a8\u7684\u4e2a\u4eba\u5934\u50cf\u88ab\u4f5c\u4e3a\u60a8\u7684\u4e2a\u4eba\u7a7a\u95f4\u7684logo\u4f7f\u7528\u3002\x3ca href\x3d\x22{0}\x22 target\x3d\x22_blank\x22\x3e\u66f4\u6539\u60a8\u7684\u4e2a\u4eba\u5934\u50cf\x3c/a\x3e.","" + '/users/profile/editmyprofilepicture.action')) + '</div><\/script></div></div>' + Confluence.Templates.Sidebar.renderSpaceToolsSection({advancedLinks: opt_data.advancedLinks, hasConfigurePermission: opt_data.hasConfigurePermission, currentlyViewed: opt_data.collectorToHighlight == 'spacebar-advanced'});
