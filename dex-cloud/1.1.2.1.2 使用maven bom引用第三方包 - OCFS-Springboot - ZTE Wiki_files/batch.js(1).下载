    var error__soy150 = opt_data.errors[0];
    output += aui.message.warning({content: '<p>' + soy.$$escapeHtml("\u4e0d\u80fd\u68c0\u7d22JIRA\u5143\u6570\u636e\u3002") + ' ' + soy.$$escapeHtml(error__soy150.errorMessage) + '</p>'});
  } else if (opt_data.errors.length > 1) {
    var param159 = '<p>' + soy.$$escapeHtml("\u4e0d\u80fd\u83b7\u53d6JIRA\u5143\u6570\u636e\u4fe1\u606f\uff0c\u53d1\u751f\u4e86\u4ee5\u4e0b\u9519\u8bef:") + '</p><ul>';
